{
    "name": "shahariaazam/zoho-oauth2",
    "description": "Zoho OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Shaharia Azam",
            "email": "mail@shaharia.com",
            "homepage": "https://github.com/shahariaazam"
        }
